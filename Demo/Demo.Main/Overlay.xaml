<Grid x:Class="Demo.Main.Overlay" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008" mc:Ignorable="d" d:DesignHeight="500"
		d:DesignWidth="800">
	<Grid.Resources>
		<SolidColorBrush x:Key="Fill" Color="#DAFFFFFF" />
	</Grid.Resources>
	<Rectangle x:Name="rectLeft" Fill="{StaticResource Fill}" HorizontalAlignment="Left" />
	<Rectangle x:Name="rectTop" Fill="{StaticResource Fill}" VerticalAlignment="Top" />
	<Rectangle x:Name="rectRight" Fill="{StaticResource Fill}" HorizontalAlignment="Right" />
	<Rectangle x:Name="rectBottom" Fill="{StaticResource Fill}" VerticalAlignment="Bottom" />
</Grid>
