<Grid x:Class="Daytimer.Controls.Ribbon.ImportControl" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:RibbonControls="clr-namespace:Daytimer.Controls.Ribbon" mc:Ignorable="d" d:DesignHeight="400"
		d:DesignWidth="700" ClipToBounds="True" Background="Transparent" Margin="40,0,58,38">
	<Grid.RowDefinitions>
		<RowDefinition Height="Auto" />
		<RowDefinition />
	</Grid.RowDefinitions>
	<TextBlock Text="Import" FontSize="40" FontFamily="/Daytimer.Fonts;component/#WeblySleek UI Light"
			Foreground="{DynamicResource BackstageSubHeaderForeground}" Grid.ColumnSpan="2" />
	<RibbonControls:BackstageWideButton x:Name="openContactButton" Grid.Row="1" Title="Open Contact"
			Description="Open a contact file in Dimension 4 (.vcf)."
			Image="pack://application:,,,/Daytimer.Images;component/Images/newcontact.png" VerticalAlignment="Top"
			HorizontalAlignment="Left" Margin="7,16,0,0" Click="openContactButton_Click" />
</Grid>
